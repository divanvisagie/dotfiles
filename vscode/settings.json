{
  "binocular.command.commands": [
    //lazygit! :)
    {
      "shellCommand": "lazygit",
      "commandIdentifier": "Lazygit"
    }
  ],
  "editor.suggestSelection": "first",
  "editor.inlineSuggest.enabled": true,
  "editor.formatOnSave": true,
  "editor.lineNumbers": "relative",
  "editor.minimap.enabled": false,
  "eslint.format.enable": true,
  "eslint.run": "onSave",
  "eslint.workingDirectories": [],
  "github.copilot.editor.enableAutoCompletions": false,
  "github.copilot.inlineSuggest.enable": true,
  "gitweblinks.bitbucketServer": [
    {
      "http": "https://stash.int.klarna.net/",
      "ssh": "git@stash.int.klarna.net"
    }
  ],
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[javascript]": {
    "editor.defaultFormatter": "dbaeumer.vscode-eslint"
  },
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[python]": {
    "editor.formatOnType": true
  },
  "[typescriptreact]": {
    "editor.defaultFormatter": "dbaeumer.vscode-eslint"
  },
  "jestrunner.codeLensSelector": "**/*.{test,spec,integration}.{js,jsx,ts,tsx}",
  "jupyter.disableJupyterAutoStart": true,
  "javascript.preferences.importModuleSpecifier": "relative",
  "outline.collapseItems": "alwaysCollapse",
  "security.workspace.trust.untrustedFiles": "open",
  "terminal.integrated.fontFamily": "MesloLGS NF",
  "terminal.integrated.shellIntegration.enabled": true,
  "typescript.preferences.importModuleSpecifier": "relative",
  "vim.leader": "<space>",
  "vim.useSystemClipboard": true,
  "vim.normalModeKeyBindings": [
    {
      "before": ["<leader>", "f", "f"],
      "commands": ["workbench.action.quickOpen"]
    },
    {
      "before": ["<leader>", "f", "s"],
      "commands": ["extension.ripgrep"]
    },
    {
      "before": ["<leader>", "i", "s"],
      "commands": ["editor.action.inlineSuggest.trigger"]
    },
    {
      "before": ["g", "d"],
      "commands": ["editor.action.goToDeclaration"]
    },
    {
      "before": ["g", "i"],
      "commands": ["editor.action.goToImplementation"]
    },
    {
      "before": ["<leader>", "s", "a"],
      "commands": ["workbench.action.toggleActivityBarVisibility"]
    },
    {
      "before": ["<leader>", "s", "c"],
      "commands": ["workbench.view.scm"]
    }
  ],
  "window.autoDetectColorScheme": true,
  "workbench.colorTheme": "GitHub Light High Contrast",
  "workbench.preferredDarkColorTheme": "green-low-contrast",
  "workbench.preferredLightColorTheme": "GitHub Light High Contrast",
  "workbench.preferredHighContrastColorTheme": "green-low-contrast",
  "yaml.schemas": {
    "file:///Users/divan.visagie/.vscode/extensions/atlassian.atlascode-3.0.2/resources/schemas/pipelines-schema.json": "bitbucket-pipelines.yml"
  },
  "application.shellEnvironmentResolutionTimeout": 30,
  "editor.guides.bracketPairsHorizontal": false,
  "editor.bracketPairColorization.enabled": false,
  "markdown-mermaid.lightModeTheme": "forest",
  "markdown-mermaid.darkModeTheme": "forest",
  "window.zoomLevel": 1,
  "workbench.activityBar.visible": false
}
